#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int i;
	string s1,s2;
	cin>>s1;
	cin>>s2;
	for(i=0;i<s1.length();i++)
	{
	    s1[i]=tolower(s1[i]);
	    s2[i]=tolower(s2[i]);
	}
	for(i=0;i<s1.length();i++)
	{
		if(s1[i]<s2[i])
		{
			cout<<-1;
			break;
		}
		else if(s1[i]>s2[i])
		{
			cout<<1;
			break;
		}
		
	}
	
	if(s1[i]==s2[i])
		{
		    cout<<0;
	
		}
	cout<<endl;
	return 0;
}
