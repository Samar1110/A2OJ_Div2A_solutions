
#include <bits/stdc++.h>

using namespace std;
typedef long long int ll;
int main()
{
    ll n;
    cin>>n;
    if(n%2==1)
    {
        cout<<-1;
    }
    else
    {
        for(ll i=1;i<=n;i++)
        {
            if(i%2==1)
            {
                cout<<i+1<<" ";
            }
            else
            {
                cout<<i-1<<" ";
            }
            
        }
    }

    return 0;
}
