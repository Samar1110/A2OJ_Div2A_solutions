#include<bits/stdc++.h>
using namespace std;
int main()
{
	string s;
	cin>>s;
	for(long long int i=0;i<s.length();i++)
	{
	    s.at(i)=tolower(s.at(i));
	}
	for(long long int i=0;i<s.length();i++)
	{
		if(s.at(i)=='a'||s.at(i)=='e'||s.at(i)=='i'||s.at(i)=='e'||s.at(i)=='o'||s.at(i)=='u'||s.at(i)=='y')
		{
			continue;
		}
		else
		{
			cout<<"."<<s.at(i);
		}
	}
	cout<<endl;
	return 0;
}
