#include <bits/stdc++.h>

using namespace std;

int main()
{
    long long int i,j,n, a,b,c,d,count1=0,count2=0,count3=0;
    cin>>a>>b;
    for(i=1;i<=6;i++)
    {
        c=abs(a-i);
        d=abs(b-i);
        if(c<d)
        count1++;
        else if(c==d)
        count2++;
        else
        count3++;
    }
    cout<<count1<<" "<<count2<<" "<<count3<<endl;
    

    return 0;
}
