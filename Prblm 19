#include <bits/stdc++.h>

using namespace std;

int main()
{
    long long int i,n,p,q,b;
    set<int> s;
    cin>>n;
    cin>>p;
    for(i=0;i<p;i++)
    {
        cin>>b;
        s.insert(b);
    }
    cin>>q;
    for(i=0;i<q;i++)
    {
        cin>>b;
        s.insert(b);
    }
    if(n==s.size())
    {
        cout<<"I become the guy."<<endl;
    }
    else
    {
        cout<<"Oh, my keyboard!"<<endl;
    }
    
    

    return 0;
}
